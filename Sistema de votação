
#include <stdio.h>
int main (){
    int i, voto, candidato1,candidato2,candidato3,n;

   printf("quantidade de eleitores participando\n");
   scanf("%d",&n);

    for(i=0;i<n;i++){
         printf("Voce deseja votar em qual candidado? [1] [2] [3]\n");
    scanf("%d",&voto);

    if(voto==1 || voto ==2 || voto==3 ){

        if(voto==1){
            candidato1++;
        }
        else if (voto==2){

            candidato2++;
        }
        else if (voto==3){

            candidato3++;
        }







    }
    }
    printf("A quantidade de votos do candidato [1] é [%d]\n",candidato1);
         printf("A quantidade de votos do candidato [2] é [%d]\n",candidato2);
         printf("A quantidade de votos do candidato [3] é [%d]\n",candidato3);





}
